{
    "variables": {
        "msg_id": {"cat": ["PART-RSP-", {"fake": ["i64", 100000000000, 999999999999]}]},
        "creation_datetime": {"fake": ["iso8601_datetime"]},
        "original_msg_id": {"cat": ["PART-CANC-", {"fake": ["i64", 100000000000, 999999999999]}]},
        "investigation_id": {"cat": ["PART-INV-", {"fake": ["i64", 1000000000, 9999999999]}]},
        "original_payment_id": {"cat": ["BULK-PMT-", {"fake": ["i64", 100000000000, 999999999999]}]},
        "original_uetr": {"fake": ["uuid_v4"]},
        "total_amount": {"fake": ["f64", 100000.0, 1000000.0]},
        "cancelled_amount": {"fake": ["f64", 25000.0, 250000.0]},
        "remaining_amount": {"fake": ["f64", 75000.0, 750000.0]},
        "original_date": {"fake": ["date", "%Y-%m-%d", "-5d", "-2d"]},
        "responder_bic": {"fake": ["bic"]},
        "originator_bic": {"fake": ["bic"]},
        "number_of_transactions": {"fake": ["i64", 5, 20]},
        "cancelled_transactions": {"fake": ["i64", 1, 5]}
    },
    "schema": {
        "Assgnmt": {
            "Id": {"var": "msg_id"},
            "Assgnr": {
                "Agt": {
                    "FinInstnId": {
                        "BICFI": {"var": "responder_bic"}
                    }
                }
            },
            "Assgne": {
                "Agt": {
                    "FinInstnId": {
                        "BICFI": {"var": "originator_bic"}
                    }
                }
            },
            "CreDtTm": {"var": "creation_datetime"}
        },
        "RsltnOfInvstgtn": {
            "Sts": {
                "Conf": "PART"
            },
            "CxlDtls": [
                {
                    "OrgnlGrpInfAndSts": {
                        "OrgnlMsgId": {"var": "original_payment_id"},
                        "OrgnlMsgNmId": "pacs.008",
                        "OrgnlNbOfTxs": {"var": "number_of_transactions"},
                        "OrgnlCtrlSum": {"var": "total_amount"},
                        "GrpCxlSts": "PART",
                        "CxlStsRsnInf": [
                            {
                                "Orgtr": {
                                    "Id": {
                                        "OrgId": {
                                            "AnyBIC": {"var": "responder_bic"}
                                        }
                                    }
                                },
                                "Rsn": {
                                    "Prtry": "PARTIAL_CANCELLATION"
                                },
                                "AddtlInf": [
                                    {"cat": [{"var": "cancelled_transactions"}, " of ", {"var": "number_of_transactions"}, " transactions cancelled"]},
                                    {"cat": ["Cancelled amount: USD ", {"var": "cancelled_amount"}]},
                                    {"cat": ["Remaining amount: USD ", {"var": "remaining_amount"}]}
                                ]
                            }
                        ]
                    },
                    "TxInfAndSts": [
                        {
                            "CxlStsId": {"cat": [{"var": "investigation_id"}, "-001"]},
                            "OrgnlInstrId": {"cat": [{"var": "original_payment_id"}, "-001"]},
                            "OrgnlEndToEndId": {"cat": ["E2E-", {"var": "original_payment_id"}, "-001"]},
                            "OrgnlUETR": {"var": "original_uetr"},
                            "TxCxlSts": "ACCR",
                            "CxlStsRsnInf": [
                                {
                                    "Rsn": {
                                        "Cd": "CUST"
                                    },
                                    "AddtlInf": ["Transaction cancelled as requested"]
                                }
                            ],
                            "OrgnlTxRef": {
                                "IntrBkSttlmAmt": {
                                    "@Ccy": "USD",
                                    "$value": {"var": "cancelled_amount"}
                                },
                                "IntrBkSttlmDt": {"var": "original_date"},
                                "Cdtr": {
                                    "Nm": {"fake": ["company_name"]}
                                },
                                "CdtrAcct": {
                                    "Id": {
                                        "IBAN": {"fake": ["iban"]}
                                    }
                                }
                            }
                        },
                        {
                            "CxlStsId": {"cat": [{"var": "investigation_id"}, "-002"]},
                            "OrgnlInstrId": {"cat": [{"var": "original_payment_id"}, "-002"]},
                            "OrgnlEndToEndId": {"cat": ["E2E-", {"var": "original_payment_id"}, "-002"]},
                            "TxCxlSts": "RJCR",
                            "CxlStsRsnInf": [
                                {
                                    "Rsn": {
                                        "Cd": "LEGL"
                                    },
                                    "AddtlInf": ["Transaction already processed - cannot cancel"]
                                }
                            ],
                            "OrgnlTxRef": {
                                "IntrBkSttlmAmt": {
                                    "@Ccy": "USD",
                                    "$value": {"fake": ["f64", 10000.0, 50000.0]}
                                },
                                "IntrBkSttlmDt": {"var": "original_date"}
                            }
                        }
                    ]
                }
            ],
            "ModDtls": {
                "OrgnlGrpInf": {
                    "OrgnlMsgId": {"var": "original_payment_id"},
                    "OrgnlMsgNmId": "pacs.008"
                },
                "InvstgtnSts": {
                    "Sts": "PART",
                    "DtTm": {"var": "creation_datetime"},
                    "Rsn": {
                        "Prtry": "PARTIAL_RESOLUTION"
                    }
                }
            }
        },
        "SplmtryData": [
            {
                "Envlp": {
                    "PartialCancellationSummary": {
                        "TotalTransactions": {"var": "number_of_transactions"},
                        "CancelledTransactions": {"var": "cancelled_transactions"},
                        "ProcessedTransactions": {"fake": ["i64", 4, 15]},
                        "AmountSummary": {
                            "TotalAmount": {"var": "total_amount"},
                            "CancelledAmount": {"var": "cancelled_amount"},
                            "ProcessedAmount": {"var": "remaining_amount"}
                        },
                        "RefundDetails": {
                            "RefundMethod": "ORIGINAL_CHANNEL",
                            "RefundInitiated": {"var": "creation_datetime"},
                            "ExpectedCompletion": {"fake": ["date", "%Y-%m-%d", "+1d", "+2d"]}
                        },
                        "NextSteps": [
                            "Partial refund will be processed within 1-2 business days",
                            "Processed transactions cannot be reversed",
                            "Contact beneficiaries directly for processed payments"
                        ]
                    }
                }
            }
        ]
    }
}
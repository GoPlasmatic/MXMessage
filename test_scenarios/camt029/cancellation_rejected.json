{
    "variables": {
        "msg_id": {"cat": ["REJ-RSP-", {"fake": ["i64", 100000000000, 999999999999]}]},
        "creation_datetime": {"fake": ["iso8601_datetime"]},
        "original_msg_id": {"cat": ["FI-CANC-", {"fake": ["i64", 100000000000, 999999999999]}]},
        "investigation_id": {"cat": ["FI-INV-", {"fake": ["i64", 1000000000, 9999999999]}]},
        "original_payment_id": {"cat": ["FI-PMT-", {"fake": ["i64", 100000000000, 999999999999]}]},
        "original_uetr": {"fake": ["uuid_v4"]},
        "payment_amount": {"fake": ["f64", 10000.0, 250000.0]},
        "original_date": {"fake": ["date", "%Y-%m-%d", "-20d", "-15d"]},
        "responder_bic": {"fake": ["bic"]},
        "originator_bic": {"fake": ["bic"]},
        "rejection_code": {"rand": ["AC01", "AC04", "AG01", "AM05", "NARR"]}
    },
    "schema": {
        "Assgnmt": {
            "Id": {"var": "msg_id"},
            "Assgnr": {
                "Agt": {
                    "FinInstnId": {
                        "BICFI": {"var": "responder_bic"}
                    }
                }
            },
            "Assgne": {
                "Agt": {
                    "FinInstnId": {
                        "BICFI": {"var": "originator_bic"}
                    }
                }
            },
            "CreDtTm": {"var": "creation_datetime"}
        },
        "RsltnOfInvstgtn": {
            "Sts": {
                "Conf": "RJCT"
            },
            "CxlDtls": [
                {
                    "TxInfAndSts": [
                        {
                            "CxlStsId": {"var": "investigation_id"},
                            "OrgnlGrpInf": {
                                "OrgnlMsgId": {"var": "original_payment_id"},
                                "OrgnlMsgNmId": "pacs.009"
                            },
                            "OrgnlInstrId": {"var": "original_payment_id"},
                            "OrgnlEndToEndId": {"cat": ["FI-E2E-", {"var": "original_payment_id"}]},
                            "OrgnlUETR": {"var": "original_uetr"},
                            "TxCxlSts": "RJCT",
                            "CxlStsRsnInf": [
                                {
                                    "Orgtr": {
                                        "Id": {
                                            "OrgId": {
                                                "AnyBIC": {"var": "responder_bic"}
                                            }
                                        }
                                    },
                                    "Rsn": {
                                        "Cd": {"var": "rejection_code"}
                                    },
                                    "AddtlInf": [
                                        {
                                            "AC01": "Account number is incorrect or invalid",
                                            "AC04": "Account closed at beneficiary bank",
                                            "AG01": "Transaction forbidden on this account type",
                                            "AM05": "Duplicate payment already processed",
                                            "NARR": "See narrative for specific rejection reason"
                                        },
                                        "Cancellation cannot be processed",
                                        "Original transaction status: FINAL"
                                    ]
                                }
                            ],
                            "OrgnlTxRef": {
                                "IntrBkSttlmAmt": {
                                    "@Ccy": "USD",
                                    "$value": {"var": "payment_amount"}
                                },
                                "IntrBkSttlmDt": {"var": "original_date"},
                                "InstgAgt": {
                                    "FinInstnId": {
                                        "BICFI": {"var": "originator_bic"}
                                    }
                                },
                                "InstdAgt": {
                                    "FinInstnId": {
                                        "BICFI": {"var": "responder_bic"}
                                    }
                                }
                            }
                        }
                    ]
                }
            ],
            "ModDtls": {
                "InvstgtnSts": {
                    "Sts": "CLOS",
                    "DtTm": {"var": "creation_datetime"},
                    "Rsn": {
                        "Cd": "RJCT",
                        "Prtry": "CANCELLATION_REJECTED"
                    }
                }
            }
        },
        "SplmtryData": [
            {
                "Envlp": {
                    "RejectionDetails": {
                        "RejectionTime": {"var": "creation_datetime"},
                        "RejectionReason": {
                            "Code": {"var": "rejection_code"},
                            "Description": {
                                "AC01": "The account number specified in the cancellation request is incorrect",
                                "AC04": "The beneficiary account has been closed",
                                "AG01": "This type of transaction is not permitted on the account",
                                "AM05": "This appears to be a duplicate of an already processed payment",
                                "NARR": "Specific rejection reason provided in narrative"
                            }
                        },
                        "RecommendedAction": "Please verify account details and transaction status with beneficiary bank"
                    }
                }
            }
        ]
    }
}
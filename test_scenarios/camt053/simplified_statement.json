{
    "variables": {
        "msg_id": {"cat": ["SIMPLE-", {"fake": ["i64", 100000000000, 999999999999]}]},
        "creation_datetime": {"fake": ["iso8601_datetime"]},
        "stmt_id": {"cat": ["BASIC-", {"fake": ["i64", 100000000000, 999999999999]}]},
        "legal_seq_number": {"fake": ["i64", 1, 999]},
        "stmt_date": {"fake": ["date", "%Y-%m-%d", "-1d", "-1d"]},
        "account_id": {"fake": ["iban"]},
        "account_owner": {"fake": ["full_name"]},
        "servicer_bic": {"fake": ["bic"]},
        "opening_balance": {"fake": ["f64", 1000.0, 50000.0]},
        "closing_balance": {"fake": ["f64", 900.0, 50100.0]},
        "total_credits": {"fake": ["f64", 500.0, 10000.0]},
        "total_debits": {"fake": ["f64", 600.0, 10100.0]}
    },
    "schema": {
        "GrpHdr": {
            "MsgId": {"var": "msg_id"},
            "CreDtTm": {"var": "creation_datetime"}
        },
        "Stmt": [
            {
                "Id": {"var": "stmt_id"},
                "LglSeqNb": {"var": "legal_seq_number"},
                "CreDtTm": {"var": "creation_datetime"},
                "FrToDt": {
                    "FrDtTm": {"cat": [{"var": "stmt_date"}, "T00:00:00Z"]},
                    "ToDtTm": {"cat": [{"var": "stmt_date"}, "T23:59:59Z"]}
                },
                "Acct": {
                    "Id": {
                        "IBAN": {"var": "account_id"}
                    },
                    "Ccy": "USD",
                    "Ownr": {
                        "Nm": {"var": "account_owner"}
                    },
                    "Svcr": {
                        "FinInstnId": {
                            "BICFI": {"var": "servicer_bic"}
                        }
                    }
                },
                "Bal": [
                    {
                        "Tp": {
                            "CdOrPrtry": {
                                "Cd": "OPBD"
                            }
                        },
                        "Amt": {
                            "@Ccy": "USD",
                            "$value": {"var": "opening_balance"}
                        },
                        "CdtDbtInd": "CRDT",
                        "Dt": {
                            "Dt": {"var": "stmt_date"}
                        }
                    },
                    {
                        "Tp": {
                            "CdOrPrtry": {
                                "Cd": "CLBD"
                            }
                        },
                        "Amt": {
                            "@Ccy": "USD",
                            "$value": {"var": "closing_balance"}
                        },
                        "CdtDbtInd": "CRDT",
                        "Dt": {
                            "Dt": {"var": "stmt_date"}
                        }
                    }
                ],
                "TxsSummry": {
                    "TtlCdtNtries": {
                        "NbOfNtries": {"fake": ["i64", 1, 5]},
                        "Sum": {"var": "total_credits"}
                    },
                    "TtlDbtNtries": {
                        "NbOfNtries": {"fake": ["i64", 1, 5]},
                        "Sum": {"var": "total_debits"}
                    }
                },
                "Ntry": [
                    {
                        "Amt": {
                            "@Ccy": "USD",
                            "$value": {"var": "total_credits"}
                        },
                        "CdtDbtInd": "CRDT",
                        "Sts": {
                            "Cd": "BOOK"
                        },
                        "BookgDt": {
                            "Dt": {"var": "stmt_date"}
                        },
                        "ValDt": {
                            "Dt": {"var": "stmt_date"}
                        },
                        "BkTxCd": {
                            "Domn": {
                                "Cd": "PMNT"
                            }
                        }
                    },
                    {
                        "Amt": {
                            "@Ccy": "USD",
                            "$value": {"var": "total_debits"}
                        },
                        "CdtDbtInd": "DBIT",
                        "Sts": {
                            "Cd": "BOOK"
                        },
                        "BookgDt": {
                            "Dt": {"var": "stmt_date"}
                        },
                        "ValDt": {
                            "Dt": {"var": "stmt_date"}
                        },
                        "BkTxCd": {
                            "Domn": {
                                "Cd": "PMNT"
                            }
                        }
                    }
                ]
            }
        ]
    }
}